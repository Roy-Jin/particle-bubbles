{
  "name": "particle-bubbles",
  "version": "1.0.2",
  "description": "A customizable particle animation library for text, shapes and images",
  "type": "module",
  "main": "dist/index.js",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "README_zh.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "particle",
    "bubbles",
    "animation",
    "canvas",
    "text",
    "shape",
    "visualization"
  ],
  "author": "Roy-Jin",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Roy-Jin/particle-bubbles.git"
  },
  "bugs": {
    "url": "https://github.com/Roy-Jin/particle-bubbles/issues"
  },
  "homepage": "https://particle-bubbles.pages.dev"
}
